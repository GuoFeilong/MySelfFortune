<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/rl_coin_container"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <include
        android:id="@+id/include_commond_title"
        layout="@layout/view_title" />

    <com.guofeilong.fortune.ui.view.CustomEditTextView
        android:id="@+id/cet_qqaite"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="50dp"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:background="@drawable/bg_budget_sub_value"
        android:paddingLeft="5dp"
        android:textColor="@color/conmmd_textcolor"
        android:textSize="14sp" />


    <ImageView
        android:id="@+id/iv_bitmap"
        android:layout_width="100dp"
        android:layout_height="50dp"
        android:layout_centerInParent="true" />

    <EditText
        android:id="@+id/et_test"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:textColor="@color/demo_title6"
        android:layout_below="@id/iv_bitmap"
        android:layout_centerHorizontal="true"
        android:layout_marginLeft="@dimen/activity_horizontal_margin"
        android:layout_marginRight="@dimen/activity_horizontal_margin"
        android:background="@drawable/bg_budget_sub_value"
        android:textSize="11sp" />

</RelativeLayout><!-- From: file:/Users/jsion/AndroidStudioPersonal/MySelfFortune/res/layout/activity_qq_aite.xml -->